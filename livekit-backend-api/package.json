{
  "name": "livekit-token-server",
  "version": "1.0.0",
  "description": "Enhanced LiveKit token server with recording capabilities",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node --esm src/index.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "migrate": "node-pg-migrate",
    "migrate:up": "node-pg-migrate up",
    "migrate:down": "node-pg-migrate down",
    "migrate:create": "node-pg-migrate create",
    "seed": "node --loader ts-node/esm src/db/seed.ts"
  },
  "keywords": [
    "livekit",
    "videoconference",
    "recording"
  ],
  "author": "Intellectif",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.490.0",
    "@aws-sdk/client-secrets-manager": "^3.490.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "livekit-server-sdk": "^2.14.0",
    "node-pg-migrate": "^7.0.0",
    "pg": "^8.11.0",
    "redis": "^4.6.0"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.19.24",
    "@types/pg": "^8.10.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.0"
  }
}
